<?xml version="1.0" encoding="UTF-8"?>
<resources>
	<!-- Transparent Theme Styles -->
	<style name="Theme.Transparent" parent="android:Theme" >
		<item name="android:windowNoTitle">true</item>
    	<item name="android:windowIsTranslucent">true</item>
    	<item name="android:windowBackground">@color/transparent</item>
    	<item name="android:backgroundDimEnabled">false</item>
    	<item name="android:windowContentOverlay">@null</item>
  	</style>
  	<style name="Theme.Transparent_75" parent="android:Theme" >
		<item name="android:windowNoTitle">true</item>
    	<item name="android:windowIsTranslucent">true</item>
    	<item name="android:windowBackground">@color/transparent_75</item>
    	<item name="android:backgroundDimEnabled">false</item>
    	<item name="android:windowContentOverlay">@null</item>
  	</style>
  	
  	<!-- Notification Theme Style -->
  	<style name="Theme.Notification" parent="android:Theme" >
		<item name="android:windowNoTitle">true</item>
    	<item name="android:windowIsTranslucent">true</item>
    	<item name="android:windowBackground">@color/limpid</item>
    	<item name="android:backgroundDimEnabled">false</item>
    	<item name="android:windowContentOverlay">@null</item>
  	</style>
  	
  	<!-- Description TextView Style -->
  	<style name="description_textview_style">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textSize">@dimen/no_result_TextSize</item>
		<item name="android:textColor">@color/no_result_text_color</item>
		<item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_marginTop">50dp</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">20dp</item>
	</style>
  	
	<!-- Title Bar Styles -->
	<style name="title_bar_layout">
		<item name="android:background">@drawable/titlebar_background</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">55dp</item>
		<item name="android:paddingLeft">5dp</item>
		<item name="android:paddingRight">5dp</item>
	</style>
	<style name="title_bar_sense_region" >
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_centerInParent">true</item>
		<item name="android:clickable">true</item>
		<item name="android:onClick">onTitleBarClick</item>
	</style>
	<style name="title_bar_image">
		<item name="android:layout_width">120dp</item>
		<item name="android:layout_height">40dp</item>
		<item name="android:src">@drawable/titlebar_image_selector</item>
		<item name="android:scaleType">fitXY</item>
		<item name="android:layout_centerInParent">true</item>
		<item name="android:clickable">false</item>
		<item name="android:duplicateParentState">true</item>
	</style>
	<style name="title_bar_text">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:paddingLeft">5dp</item>
		<item name="android:paddingRight">5dp</item>
		<item name="android:maxWidth">220dp</item>
		<item name="android:textSize">18sp</item>
		<item name="android:textColor">@drawable/color_white_limpid_selector</item>
		<item name="android:textStyle">bold</item>
		<item name="android:lines">1</item>
		<item name="android:gravity">center</item>
		<item name="android:layout_centerInParent">true</item>
		<item name="android:duplicateParentState">true</item>
	</style>
	<style name="title_bar_back">
		<item name="android:layout_width">42dp</item>
		<item name="android:layout_height">38dp</item>
		<item name="android:paddingLeft">8dp</item>
		<item name="android:paddingRight">8dp</item>
		<item name="android:paddingTop">6dp</item>
		<item name="android:paddingBottom">6dp</item>
		<item name="android:background">@drawable/bar_button_selector</item>
		<item name="android:scaleType">fitXY</item>
		<item name="android:layout_centerVertical">true</item>
	</style>
	<style name="title_bar_next">
		<item name="android:layout_width">42dp</item>
		<item name="android:layout_height">38dp</item>
		<item name="android:paddingLeft">8dp</item>
		<item name="android:paddingRight">8dp</item>
		<item name="android:paddingTop">6dp</item>
		<item name="android:paddingBottom">6dp</item>
		<item name="android:background">@drawable/bar_button_selector</item>
		<item name="android:scaleType">fitXY</item>
		<item name="android:layout_centerVertical">true</item>
		<item name="android:layout_alignParentRight">true</item>
	</style>
	
	<!-- Option Tag Styles -->
	<style name="option_tag_layout">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">50dp</item>
		<item name="android:layout_marginTop">-5dp</item>
		<item name="android:layout_marginLeft">-5dp</item>
		<item name="android:layout_marginRight">-5dp</item>
		<item name="android:background">@drawable/option_bar</item>
	</style>
	<style name="option_tag_content_layout">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">fill_parent</item>
		<item name="android:orientation">horizontal</item>
		<item name="android:weightSum">3</item>
	</style>
	<style name="option_text">
		<item name="android:layout_width">0dp</item>
		<item name="android:layout_height">fill_parent</item>
		<item name="android:layout_weight">1</item>
		<item name="android:textSize">17sp</item>
		<item name="android:textColor">@color/orange</item>
		<item name="android:gravity">center</item>
	</style>
	<style name="option_frame">
		<item name="android:layout_width">0dp</item>
		<item name="android:layout_height">fill_parent</item>
		<item name="android:layout_weight">1.12</item>
		<item name="android:layout_marginLeft">-5dp</item>
		<item name="android:background">@drawable/option_select</item>
	</style>
	<style name="option_image">
		<item name="android:layout_width">42dp</item>
		<item name="android:layout_height">42dp</item>
		<item name="android:scaleType">fitXY</item>
		<item name="android:layout_gravity">center</item>
	</style>
	
	<!-- Avatar Styles -->
	<style name="avatar">
        <item name="android:padding">5dp</item>
        <item name="android:background">@drawable/avatar_frame</item>
        <item name="android:src">@drawable/avatar_loading</item>
        <item name="android:scaleType">fitXY</item>
	</style>
	<style name="avatar_small">
        <item name="android:padding">3dp</item>
        <item name="android:background">@drawable/avatar_frame_small</item>
        <item name="android:src">@drawable/avatar_loading</item>
        <item name="android:scaleType">fitXY</item>
	</style>
	<style name="program">
        <item name="android:padding">5dp</item>
        <item name="android:background">@drawable/avatar_frame</item>
        <item name="android:src">@drawable/program_loading</item>
        <item name="android:scaleType">fitXY</item>
	</style>
	<style name="program_small">
        <item name="android:padding">3dp</item>
        <item name="android:background">@drawable/avatar_frame_small</item>
        <item name="android:src">@drawable/program_loading</item>
        <item name="android:scaleType">fitXY</item>
	</style>
	
	<!-- Topfan Styles -->
	<style name="topfan_score_box_topfan">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">28dp</item>
		<item name="android:textSize">15sp</item>
		<item name="android:textColor">@color/orange</item>
		<item name="android:textStyle">bold</item>
		<item name="android:layout_marginBottom">5dp</item>
		<item name="android:gravity">left|center_vertical</item>
	</style>
    
	<style name="topfan_score_box">
		<item name="android:layout_width">56dp</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textSize">11sp</item>
		<item name="android:textColor">@color/topfan_score</item>
		<item name="android:textStyle">bold</item>
		<item name="android:gravity">center</item>
	</style>
	
	<style name="topfan_points_text">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textSize">9sp</item>
		<item name="android:textColor">@color/orange</item>
	</style>
	
	
	<!-- main & sign in & sign up    title bar styles -->
	<style name="main_title_bar_layout">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">65dp</item>
		<item name="android:paddingLeft">5dp</item>
		<item name="android:paddingRight">5dp</item>
	</style>
	<style name="main_title_bar_back">
		<item name="android:layout_width">50dp</item>
		<item name="android:layout_height">50dp</item>
		<item name="android:scaleType">fitXY</item>
		<item name="android:layout_centerVertical">true</item>
	</style>
	<style name="main_title_bar_logo">
		<item name="android:layout_width">125dp</item>
		<item name="android:layout_height">38dp</item>
		<item name="android:src">@drawable/fanwave_logo_small</item>
		<item name="android:scaleType">fitXY</item>
		<item name="android:layout_centerInParent">true</item>
	</style>
	<style name="main_title_bar_next">
		<item name="android:layout_width">50dp</item>
		<item name="android:layout_height">50dp</item>
		<item name="android:scaleType">fitXY</item>
		<item name="android:layout_centerVertical">true</item>
		<item name="android:layout_alignParentRight">true</item>
	</style>
	
	
	<!-- main_page functions icon style -->
	<style name="main_function_icon_linearlayout" >
		<item name="android:layout_width">0dp</item>
		<item name="android:layout_height">fill_parent</item>
		<item name="android:layout_weight">1</item>
		<item name="android:orientation">vertical</item>
		<item name="android:gravity">center</item>
		<item name="android:clickable">true</item>
		<item name="android:clipChildren">true</item>
	</style>
	
	<style name="main_function_icon_image" >
		<item name="android:layout_width">100dp</item>
		<item name="android:layout_height">80dp</item>
		<item name="android:scaleType">fitXY</item>
		<item name="android:duplicateParentState">true</item>
	</style>
	
	<style name="main_function_icon_text" >
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:gravity">top</item>
		<item name="android:maxWidth">140dp</item>
		<item name="android:textSize">@dimen/main_page_TextSize</item>
		<item name="android:textColor">@color/main_page_text_color</item>
		<item name="android:textStyle">bold</item>
		<item name="android:paddingTop">1dp</item>
		<item name="android:lines">2</item>
		<item name="android:layout_marginLeft">-10dp</item>
		<item name="android:duplicateParentState">true</item>
	</style>
	
	<!-- List Row Divider Styles -->
	<style name="list_divider">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">2dp</item>
		<item name="android:src">@drawable/list_divider</item>
	</style>
	<style name="list_divider_vertical">
		<item name="android:layout_width">2dp</item>
		<item name="android:layout_height">fill_parent</item>
		<item name="android:src">@drawable/list_divider_vertical</item>
	</style>
	
	<!-- Extrainfo Segment Styles -->
	<style name="extrainfo_segment_linear">
		<item name="android:orientation">horizontal</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">50dp</item>
		<item name="android:paddingLeft">10dp</item>
		<item name="android:paddingRight">10dp</item>
		<item name="android:paddingTop">5dp</item>
		<item name="android:paddingBottom">5dp</item>
		<item name="android:background">@drawable/tabbar_extrainfo_background</item>
		<item name="android:gravity">center</item>
	</style>
	<style name="extrainfo_segment_item">
		<item name="android:layout_height">fill_parent</item>
		<item name="android:textSize">18sp</item>
		<item name="android:textColor">@drawable/color_dark_brown_white_selector</item>
		<item name="android:textStyle">bold</item>
		<item name="android:gravity">center</item>
	</style>
	
	<!-- Personal ExtraInfo Styles -->
	<style name="detail_layout">
		<item name="android:orientation">horizontal</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">60dp</item>
		<item name="android:gravity">center_vertical</item>
		<item name="android:paddingLeft">5dp</item>
		<item name="android:paddingRight">5dp</item>
	</style>
	<style name="detail_icon">
		<item name="android:layout_width">30dp</item>
		<item name="android:layout_height">30dp</item>
		<item name="android:gravity">center</item>
		<item name="android:layout_marginLeft">10dp</item>
		<item name="android:scaleType">centerInside</item>
	</style>
	<style name="detail_item">
		<item name="android:layout_width">60dp</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_marginLeft">5dp</item>
		<item name="android:textSize">14sp</item>
		<item name="android:textColor">@color/gray</item>
	</style>
	<style name="detail_colon">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:text">:</item>
		<item name="android:textSize">14sp</item>
		<item name="android:textColor">@color/gray</item>
		<item name="android:layout_marginRight">5dp</item>
	</style>
	<style name="detail_value">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textSize">14sp</item>
		<item name="android:textColor">@color/gray</item>
		<item name="android:singleLine">true</item>
	</style>
	
	<!-- Search Bar Styles -->
	<style name="search_bar_layout">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">50dp</item>
		<item name="android:paddingLeft">5dp</item>
		<item name="android:paddingRight">5dp</item>
		<item name="android:background">@drawable/post_background</item>
		<item name="android:gravity">center_vertical</item>
		<item name="android:focusableInTouchMode">true</item>
		<item name="android:weightSum">1</item>
	</style>
	<style name="search_bar">
		<item name="android:layout_width">0dp</item>
		<item name="android:layout_weight">1</item>
		<item name="android:layout_height">fill_parent</item>
		<item name="android:layout_marginTop">3dp</item>
		<item name="android:layout_marginBottom">1dp</item>
		<item name="android:textColor">@color/black</item>
		<item name="android:textSize">14sp</item>
	</style>
	<style name="search_button">
		<item name="android:src">@drawable/search_selector</item>
		<item name="android:layout_width">38dp</item>
		<item name="android:layout_height">38dp</item>
		<item name="android:layout_marginLeft">5dp</item>
		<item name="android:scaleType">fitXY</item>
		<item name="android:layout_gravity">center</item>
	</style>
	
</resources>