<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" 
	android:background="@drawable/fanwave_background_white" >
	<!-- Title Bar Layout -->
	<RelativeLayout android:id="@+id/title_bar_layout" style="@style/title_bar_layout">
			
		<RelativeLayout android:id="@+id/title_sense_region"
            style="@style/title_bar_sense_region" >
            <!-- Title Image View -->
            <ImageView android:id="@+id/title_imageview"
            style="@style/title_bar_image" />
            
            <!-- Title Text View -->	
            <TextView android:id="@+id/title_textview"
			style="@style/title_bar_text"
			android:text="@string/guides_epg_setting" />
        </RelativeLayout>
				
		<!-- Back Image Button -->
		<ImageButton android:id="@+id/back_imagebutton" 
			style="@style/title_bar_back"
			android:src="@drawable/guide_icon" />
				
	</RelativeLayout>	
	

	<!-- android:background="@drawable/account_settings_activity_linearlayout_shape" -->
	<LinearLayout 
		android:layout_width="fill_parent" 
		android:layout_height="fill_parent" 
		android:orientation="vertical" 
		android:paddingBottom="20dip" 
        android:layout_marginTop="-3dp">
		<LinearLayout 
			android:layout_width="fill_parent" 
			android:layout_height="wrap_content" 
			android:orientation="vertical" >
			
			<RelativeLayout android:id="@+id/my_provider_relativelayout" 
				android:layout_width="fill_parent" 
				android:layout_height="50dp" 
				android:clickable="true" 
				android:background="@color/white" >
				
				<ImageView android:id="@+id/my_provider_imageview"
					android:layout_width="50dip" 
					android:layout_height="fill_parent" 
					android:paddingLeft="12dp"
					android:paddingRight="12dp" 
					android:paddingTop="14dp"
					android:paddingBottom="14dp"
					android:src="@drawable/guide_provider" 
					android:scaleType="fitXY" 
					android:duplicateParentState="true" />
				<TextView 
					android:layout_width="wrap_content" 
					android:layout_height="fill_parent" 
					android:text="@string/guides_program_setting_myprovider" 
					android:textColor="@drawable/textcolor_gray_orange_selector" 
					android:textSize="17dip" 
					android:textStyle="bold" 
					android:gravity="center_vertical" 
					android:layout_centerVertical="true" 
					android:layout_toRightOf="@+id/my_provider_imageview" 
					android:duplicateParentState="true" />

			</RelativeLayout>
			
			<ImageView 
				android:layout_width="fill_parent" 
				android:layout_height="wrap_content" 
				android:background="@drawable/list_divider" />
			
			<RelativeLayout  android:id="@+id/channel_reorder_relativelayout"
				android:layout_width="fill_parent" 
				android:layout_height="50dip" 
				android:clickable="true" 
				android:background="@color/white" >
				<ImageView android:id="@+id/channel_reorder_imageview"
					android:layout_width="50dip" 
					android:layout_height="fill_parent" 
					android:paddingLeft="12dp"
					android:paddingRight="12dp" 
					android:paddingTop="14dp"
					android:paddingBottom="14dp"
					android:src="@drawable/guide_reorder" 
					android:scaleType="fitXY" 
					android:duplicateParentState="true" />
				<TextView 
					android:layout_width="wrap_content" 
					android:layout_height="fill_parent" 
					android:text="@string/guides_program_setting_channelreorder" 
					android:textColor="@drawable/textcolor_gray_orange_selector"  
					android:textSize="17dip" 
					android:textStyle="bold" 
					android:gravity="center_vertical" 
					android:layout_centerVertical="true" 
					android:layout_toRightOf="@+id/channel_reorder_imageview" 
					android:duplicateParentState="true" />

			</RelativeLayout>
			
			<ImageView 
				android:layout_width="fill_parent" 
				android:layout_height="wrap_content" 
				android:background="@drawable/list_divider" />
			
			<RelativeLayout  android:id="@+id/channel_hidder_relativelayout"
				android:layout_width="fill_parent" 
				android:layout_height="50dip" 
				android:clickable="true" 
				android:background="@color/white" >
				<ImageView android:id="@+id/channel_hidder_imageview"
					android:layout_width="50dip" 
					android:layout_height="fill_parent" 
					android:paddingLeft="12dp"
					android:paddingRight="12dp" 
					android:paddingTop="14dp"
					android:paddingBottom="14dp"
					android:src="@drawable/guide_hidder" 
					android:scaleType="fitXY" 
					android:duplicateParentState="true" />
				<TextView 
					android:layout_width="wrap_content" 
					android:layout_height="fill_parent" 
					android:text="@string/guides_program_setting_channelhidder" 
					android:textColor="@drawable/textcolor_gray_orange_selector"  
					android:textSize="17dip" 
					android:textStyle="bold" 
					android:gravity="center_vertical" 
					android:layout_centerVertical="true" 
					android:layout_toRightOf="@+id/channel_hidder_imageview" 
					android:duplicateParentState="true" />

			</RelativeLayout>
			<ImageView 
				android:layout_width="fill_parent" 
				android:layout_height="wrap_content" 
				android:background="@drawable/list_divider" />
			
			
		</LinearLayout>
		
	</LinearLayout>

</LinearLayout>